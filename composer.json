{
  "name": "n0wada/chert",
  "type": "library",
  "description": "It is a simple routing library using annotations and caching.",
  "keywords": [
    "silex",
    "provider",
    "annotation",
    "routing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Naoto Owada",
      "email": "naoto0wada@gmail.com"
    }
  ],
  "require": {
    "symfony/finder": "~2.8|^3.0",
    "doctrine/annotations": "~1.0",
    "doctrine/cache": "~1.0"
  },
  "require-dev": {
    "silex/silex": "~2.0",
    "symfony/browser-kit": "~2.8|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Chert\\": "src/chert"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  }
}
